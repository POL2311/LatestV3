{
  "name": "@gasless-infra/sdk",
  "version": "1.0.2",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup src/index.ts --dts --format cjs,esm",
    "clean": "rm -rf dist",
    "pack": "npm run build && npm pack"
  },
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.3.3",
    "@types/node": "^20.10.5"
  },
  "dependencies": {
    "@solana/web3.js": "^1.87.6",
    "axios": "^1.6.2",
    "bs58": "^5.0.0",
    "tweetnacl": "^1.0.3",
    "@solana/wallet-adapter-base": "^0.9.23"
  }
}
